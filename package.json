{
  "name": "whats-cookin",
  "version": "1.0.0",
  "description": "Simple recipe sharing app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:server-dev": "cd server && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:server": "cd server && npm install && node server.js",
    "start:client": "cd client && npm install && npm start",
    "start:dev": "concurrently \"npm run start:server-dev\" \"npm run start:client\"",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "author": "Tiffany Boorsma",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "concurrently": "^3.5.1",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.1.0",
    "express-jwt-authz": "^1.0.0",
    "jwks-rsa": "^1.1.1",
    "mongoose": "^5.0.16",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "nodemon": "^1.17.3"
  }
}
